
// dev の圧縮
$ssh a
$zip -ru ./ontheroad.jp/dev.zip ./ontheroad.jp/public_html/dev

// sftp で取得
$sftp -i ~/.ssh/id_rsa -P 10022 xiaozhe@sv80.xserver.jp
$get ./ontheroad.jp/dev.zip
$bye

// SQL バックアップ
$mysqldump -u xiaozhe_wpdev -pbunnta0421 -h mysql9.xserver.jp --all-databases | gzip > ./ontheroad.jp/devsql.gz



